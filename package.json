{
  "name": "react-mobile-boilerplate",
  "version": "0.4.2",
  "description": "A react boilerplate for mobile webapp",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node src/server/main.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/server/main.js",
    "start:hot": "cross-env NODE_ENV=development babel-node webpack/webpack.devServer.js",
    "dev": "concurrently --kill-others \"npm run start:hot\" \"npm run start:dev\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config webpack/webpack.config.server.js",
    "profile": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --profile --json > profile.json",
    "clean": "rimraf build && rimraf public",
    "eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-mobile-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "mobile"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-mobile-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-mobile-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "postcss": "^5.2.17",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.1",
    "postcss-simple-vars": "^3.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "hahoorequest": "^0.6.2",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.4",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tappable": "^0.8.4",
    "redux": "^3.6.0",
    "redux-act-reducer": "^2.1.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "webpack-isomorphic-tools": "^3.0.2"
  }
}
