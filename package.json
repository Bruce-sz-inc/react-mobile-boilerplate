{
  "name": "react-mobile-boilerplate",
  "version": "0.2.2",
  "description": "A react boilerplate for mobile webapp",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production babel-node src/server/main.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/server/main.js",
    "start:hot": "cross-env NODE_ENV=development babel-node webpack/webpack.devServer.js",
    "dev": "concurrently --kill-others \"npm run start:hot\" \"npm run start:dev\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config webpack/webpack.config.server.js",
    "profile": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --profile --json > profile.json",
    "clean": "rimraf build && rimraf public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-mobile-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "mobile"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-mobile-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-mobile-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "clean-webpack-plugin": "^0.1.13",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.24.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.0",
    "postcss-mixins": "^5.4.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "hahoorequest": "^0.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-act-reducer": "^1.5.2",
    "redux-thunk": "^2.1.0",
    "webpack-isomorphic-tools": "^2.6.3"
  }
}
