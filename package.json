{
  "name": "react-mobile-boilerplate",
  "version": "0.6.0",
  "description": "A react boilerplate for mobile webapp",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node src/server/main.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/server/main.js",
    "start:hot": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "dev": "concurrently --kill-others \"npm run start:hot\" \"npm run start:dev\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config webpack/webpack.config.server.js",
    "profile": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --profile --json > profile.json",
    "clean": "rimraf build && rimraf public",
    "eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-mobile-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "mobile"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-mobile-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-mobile-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^2.30.1",
    "postcss": "^6.0.16",
    "postcss-loader": "^2.0.10",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.1.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "hahoorequest": "^0.6.7",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-act-reducer": "^2.1.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "webpack-isomorphic-tools": "^3.0.5"
  }
}
