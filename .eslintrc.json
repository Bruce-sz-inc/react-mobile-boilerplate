{
   "extends": "airbnb",
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "comma-dangle": ["warn", "only-multiline"],
        "indent": [2, 2, {"SwitchCase": 1}],
        "no-console": 0,
        "no-alert": 0
    },
    "globals": {
      "window": true
    }
}
